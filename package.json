{
  "name": "storeon-velo",
  "version": "4.1.0",
  "description": "A tiny event-based state manager Storeon for Velo by Wix",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./legacy": {
      "require": "./legacy/index.cjs",
      "import": "./legacy/index.js",
      "default": "./legacy/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src/**/*.js tests/**/*.js",
    "build": "rollup -c",
    "size": "size-limit",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/jest": "^29.4.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "rollup": "^3.16.0",
    "size-limit": "^8.2.4"
  },
  "size-limit": [
    {
      "path": "lib/index.cjs",
      "import": "{ createStoreon }",
      "limit": "659 B"
    },
    {
      "path": "lib/index.js",
      "import": "{ createStoreon }",
      "limit": "368 B"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shoonia/storeon-velo.git"
  },
  "bugs": {
    "url": "https://github.com/shoonia/storeon-velo/issues"
  },
  "keywords": [
    "wix",
    "corvid",
    "wixcode",
    "editorx",
    "velo",
    "storeon",
    "redux"
  ],
  "files": [
    "lib",
    "legacy",
    "src",
    "index.d.ts"
  ],
  "author": {
    "name": "Alexander Zaytsev",
    "email": "alexanderz@wix.com",
    "url": "https://twitter.com/_shoonia"
  },
  "homepage": "https://github.com/shoonia/storeon-velo#readme",
  "license": "MIT",
  "private": false
}
