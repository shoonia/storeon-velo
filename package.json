{
  "name": "corvid-storeon",
  "version": "2.2.0",
  "description": "A tiny event-based state manager Storeon for Corvid by Wix",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "lint-js": "eslint src/** dist/** tests/**",
    "lint-md": "markdownlint '**/*.md' --ignore node_modules",
    "dist": "rollup --config rollup.config.js && eslint dist/** --fix",
    "test": "jest tests/**.test.js"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "markdownlint": "^0.19.0",
    "markdownlint-cli": "^0.22.0",
    "rollup": "^2.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "storeon": "^1.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run dist && npm run lint-js && npm run lint-md && npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shoonia/corvid-storeon.git"
  },
  "bugs": {
    "url": "https://github.com/shoonia/corvid-storeon/issues"
  },
  "keywords": [
    "wix",
    "corvid",
    "corvid-cli",
    "wixcode",
    "storeon",
    "redux"
  ],
  "author": {
    "name": "Alexander Zaytsev",
    "email": "alexanderz@wix.com",
    "url": "https://twitter.com/_shoonia"
  },
  "homepage": "https://github.com/shoonia/corvid-storeon#readme",
  "license": "MIT",
  "private": false
}
