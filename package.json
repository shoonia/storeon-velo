{
  "name": "storeon-velo",
  "version": "1.6.0",
  "description": "A tiny event-based state manager Storeon for Velo by Wix",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src/**/*.js tests/**/*.js",
    "build": "rollup -c && eslint lib/**/*.js --fix",
    "size": "size-limit",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@size-limit/preset-small-lib": "^7.0.3",
    "@types/jest": "^27.0.3",
    "eslint": "^8.4.1",
    "jest": "^27.4.4",
    "rollup": "^2.61.1",
    "simple-git-hooks": "^2.7.0",
    "size-limit": "^7.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint && npm run build && npm test && npm run size"
  },
  "size-limit": [
    {
      "path": "lib/index.cjs",
      "import": "{ createStoreon }",
      "limit": "754 B"
    },
    {
      "path": "lib/index.js",
      "import": "{ createStoreon }",
      "limit": "442 B"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shoonia/storeon-velo.git"
  },
  "bugs": {
    "url": "https://github.com/shoonia/storeon-velo/issues"
  },
  "keywords": [
    "wix",
    "corvid",
    "wixcode",
    "editorx",
    "velo",
    "storeon",
    "redux"
  ],
  "files": [
    "lib",
    "src",
    "LICENSE",
    "README.md"
  ],
  "author": {
    "name": "Alexander Zaytsev",
    "email": "alexanderz@wix.com",
    "url": "https://twitter.com/_shoonia"
  },
  "homepage": "https://github.com/shoonia/storeon-velo#readme",
  "license": "MIT",
  "private": false
}
