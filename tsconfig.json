{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "module": "ES2022",
    "moduleResolution": "Node",
    "target": "ES2022",
    "sourceMap": false,
    "noEmit": true,
    "lib": [
      "ES2022"
    ]
  },
  "include": [
    "src/**/*.js",
    "tests/**/*.js",
    "tests/legacy/$w.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
